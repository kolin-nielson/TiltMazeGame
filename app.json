{
  "expo": {
    "name": "Tilt Maze",
    "slug": "the-tilt-maze",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/9dd5d0d0-2b31-4a44-9710-feec4b2a217f"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kolinnielson.tiltmaze",
      "buildNumber": "6",
      "infoPlist": {
        "NSMotionUsageDescription": "This app uses motion sensors to control the game through device tilting.",
        "UIRequiresFullScreen": true,
        "UIStatusBarHidden": true,
        "ITSAppUsesNonExemptEncryption": false,
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationPortrait"
        ],
        "UISupportedInterfaceOrientations~ipad": [
          "UIInterfaceOrientationPortrait"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kolinnielson.tiltmaze",
      "versionCode": 3,
      "permissions": [
        "android.permission.VIBRATE"
      ],
      "softwareKeyboardLayoutMode": "pan",
      "allowBackup": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-sensors",
        {
          "motionPermission": "Allow $(PRODUCT_NAME) to access your device motion to control the game."
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized ads to you.",
          "androidAppId": "ca-app-pub-4299404428269280~8903477157",
          "iosAppId": "ca-app-pub-4299404428269280~6018845391",
          "skipProguardConfigGeneration": false
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9dd5d0d0-2b31-4a44-9710-feec4b2a217f"
      }
    }
  }
}
